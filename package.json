  "scripts": {
    "dev-server": "webpack-dev-server --mode=development --open --hot --progress",
    "dev": "concurrently \"npm run start\" \"npm run start:react\"",
    "build": "webpack --mode=production --progress",
    "start": "nodemon ./server/index.js",
    "start:react": "webpack --mode=development --progress",
    "test": "jest --watch",
    "coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
